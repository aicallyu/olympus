{
  "name": "mission-control-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > src/db/types.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.0",
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^16.3.0",
    "hono": "^4.0.0",
    "pg": "^8.18.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.16.0",
    "jsdom": "^28.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  }
}
